/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a, out=aNot);
    Not(in=b, out=bNot);
    And(a=aNot, b=b, out=aNotAndb);
    And(a=a, b=bNot, out=aAndbNot);
    Or(a=aNotAndb, b=aAndbNot, out=out);
}